<Window x:Class="TriviaClient.EndGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TriviaClient"
        mc:Ignorable="d"
        Title="EndGameWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name ="Btn_back" Content="Back"  HorizontalAlignment="Left" Margin="669,26,0,349" Width="100" Click="Btn_back_Click"/>
        <TextBlock HorizontalAlignment="Center" Margin="192,43,205,0" TextWrapping="Wrap" Text="ScoreBoard" VerticalAlignment="Top" Height="85" Width="395" TextAlignment="Center" FontSize="30" Foreground="White"/>
        <Grid x:Name="mainPart" HorizontalAlignment="Right" Height="370" Margin="0,110,89,-61" VerticalAlignment="Top" Width="630" RenderTransformOrigin="0.492,0.392">
            <Grid.RowDefinitions>
                <RowDefinition Height="11*"/>
                <RowDefinition Height="63*"/>
            </Grid.RowDefinitions>
        </Grid>
    </Grid>
    <Window.Background>
        <ImageBrush ImageSource="endGame.png" Stretch="UniformToFill"/>
    </Window.Background>
</Window>
